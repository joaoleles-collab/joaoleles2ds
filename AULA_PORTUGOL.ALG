Algoritmo "aula_portugol"

Var
// Seção de Declarações das variáveis 
// TIPOS DE DADOS
// caractere - texto
nome: caractere
// real - decimal, numeros quebrados
valor: real
// inteiro - "valor redondo"
idade: inteiro
// logico - verdadeiro e falso, booleani
ligado: logico

  //regras de variaveis
  // 1 - não pode palavras dedicadas
